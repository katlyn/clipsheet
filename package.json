{
  "name": "clipsheet",
  "version": "0.0.1",
  "description": "Pull data from public Google Sheets using Google Query Language",
  "keywords": [
    "Google Sheets",
    "spreadsheet",
    "sheet",
    "sheetrock"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup --dts",
    "prepublishOnly": "pnpm build",
    "prepare": "husky",
    "test": "tap run"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "Katlyn Lorimer <opensource@katlyn.dev>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tap": "^18.8.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "undici": "^6.18.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint .",
      "prettier --write --ignore-unknown"
    ],
    "*.json": "prettier --write --ignore-unknown"
  },
  "type": "module"
}
